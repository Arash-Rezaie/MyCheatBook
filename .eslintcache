mport {Circle} from \"../simple/circle\";\nimport {Text} from \"../simple/text\";\nimport {Vector} from \"../simple/line\";\nimport {Caption} from \"./caption\";\nimport {ExtendedVector} from \"./extended-line\";\n\nexport class Node extends Circle {\n    constructor(handler) {\n        super();\n        this.handler = handler;\n        this.setRadius(20)\n            .setStrokeWidth(3)\n            .setStrokeColor('black')\n            .setFillColor('yellow');\n    }\n\n    setLabel(label) {\n        if (typeof label === 'string') {\n            label = new Text()\n                .setPosition(this.getCenterPoint())\n                .setLabel(label)\n                .setFontSize('20px')\n                .setOffset([0, 6]);\n        } else {\n            label = this.handler.generateShape(label);\n        }\n        return super.setLabel(label);\n    }\n\n    connectTo(refId) {\n        this.handler.addDirectShape(new ExtendedVector(this.handler)\n            .between(this, this.handler.getTargetShape(refId), 5));\n        return this;\n    }\n\n    setCaptions(caps) {\n        if (caps[0] === undefined)\n            caps = [caps];\n        caps.forEach((v, i) => {\n            this.handler.addDirectShape(\n                new Caption(this.handler)\n                    .alignTo(this)\n                    .setOffset(v.offset ? v.offset : [28, (caps.length * -11.5 + (i * 25))])\n                    .setWidth(v.w)\n                    .setHeight(20)\n                    .setLabel(v.t)\n                    .setColor(v.c)\n                    .setStrokeWidth(2)\n            )\n        });\n        return this;\n    }\n}","/media/arash/SanDisk/MyCheatBook/src/components/drawbox/extended/caption.js",["184"],"import {Rectangle} from \"../simple/rectangle\";\nimport {Text} from \"../simple/text\";\nimport {Line} from \"../simple/line\";\nimport {ExtendedLine} from \"./extended-line\";\n\nexport class Caption extends Rectangle {\n    constructor(handler) {\n        super();\n        this.handler = handler;\n    }\n\n    setLabel(label) {\n        if (typeof label === 'string') {\n            label = new Text()\n                .setPosition(this.getCenterPoint())\n                .setLabel(label)\n                .setOffset([0, 5])\n                .setFontStyle('bold');\n        } else {\n            label = this.handler.generateShape(label);\n        }\n        return super.setLabel(label);\n    }\n\n    connectTo(refId) {\n        this.drawBox.addDirectShape(new ExtendedLine(this.handler)\n            .between(this, this.handler.getTargetShape(refId), 5)\n            .setStrokeColor('red')\n            .setFillColor('red')\n            .setStrokeWidth(1));\n        return this;\n    }\n}",{"ruleId":"185","severity":1,"message":"186","line":6,"column":9,"nodeType":"187","messageId":"188","endLine":6,"endColumn":16},{"ruleId":"185","severity":1,"message":"189","line":13,"column":10,"nodeType":"187","messageId":"188","endLine":13,"endColumn":24},{"ruleId":"185","severity":1,"message":"190","line":18,"column":9,"nodeType":"187","messageId":"188","endLine":18,"endColumn":11},{"ruleId":"185","severity":1,"message":"191","line":19,"column":9,"nodeType":"187","messageId":"188","endLine":19,"endColumn":11},{"ruleId":"185","severity":1,"message":"192","line":20,"column":9,"nodeType":"187","messageId":"188","endLine":20,"endColumn":11},{"ruleId":"193","replacedBy":"194"},{"ruleId":"195","replacedBy":"196"},{"ruleId":"185","severity":1,"message":"197","line":5,"column":37,"nodeType":"187","messageId":"188","endLine":5,"endColumn":51},{"ruleId":"185","severity":1,"message":"198","line":3,"column":9,"nodeType":"187","messageId":"188","endLine":3,"endColumn":15},{"ruleId":"185","severity":1,"message":"199","line":3,"column":9,"nodeType":"187","messageId":"188","endLine":3,"endColumn":13},"no-unused-vars","'DrawBox' is defined but never used.","Identifier","unusedVar","'getMainContent' is defined but never used.","'p1' is assigned a value but never u